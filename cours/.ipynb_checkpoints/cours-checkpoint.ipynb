   "source": [
    "def set_age(age):\n",
    "    if age < 0:\n",
    "        raise ValueError(\"L'âge ne peut pas être négatif.\")\n",
    "    print(f\"Âge défini à {age} ans.\")\n",
    "\n",
    "try:\n",
    "    set_age(10)\n",
    "except ValueError as e:\n",
    "    print(e)"
   ]
